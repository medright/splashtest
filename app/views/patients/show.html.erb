<h1 class="code">Code Status: <%= @patient.code %></h1>

<nav>
  <%= link_to("all patients", patients_path) %>
  <%= link_to("edit", edit_patient_path(@patient)) %>
  <%= link_to "delete", patient_path(@patient), method: :delete, data: { confirm: 'Are you sure?'} %>
</nav>
  
<div class="edits_list" >
<ul>
	<li><%= @patient.first_name %>, <%= @patient.last_name %> <%= @patient.dob %> 
	<%= @patient.allergies %> <%= @patient.code %> <%= @patient.notes %></li>
</ul>
</div>


