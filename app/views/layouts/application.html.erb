<!DOCTYPE html>
<html>
<head>
  <title>(evr)</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

		
		<div class="topbar">
			<div class="toptile">
				<p>John Doe<br>
			  12/24/82<br>
			  allergy: sulfa, codeine.<br>
			  Primary: Dr Doe<br>
			  Surgeon: Dr Mallon<br> 
			</p>
			</div>
			<div class="toptile">
				notes
			</div>
			<div class="toptile">
				labs
			</div>
			<div class="toptile">
				VS
			</div>
			<div class="toptile">
			  <%= link_to("Add Med", new_med_path) %> 
			  <%= link_to("Meds list", root_path) %>
			  <%= link_to("Pt's list", patients_path) %> 
			  <%= link_to("Add Pt", new_patient_path) %>
			  <%= link_to("Notes list", notes_path) %>
			  <%= link_to("Add Note", new_note_path) %>
			</div>
		</div>
		

		

		<div class="rtbar">
			<div class="rttile">
				start stream
			</div>
			<div class="rttile">
				download chart
			</div>
			<div class="rttile">
				upload image
			</div>
		</div>

		<div class="content">
			<%= yield %>
		</div>

	</body>
</html>
