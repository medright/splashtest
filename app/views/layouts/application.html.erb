<!DOCTYPE html>
<html>
<head>
  <title>(evr)</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

		
		<div class="topbar">
			<div class="toptile">
				<button><%= link_to("Pt's list", patients_path) %></button> 
			  <button><%= link_to("Add Pt", new_patient_path) %></button>
			  User: Dr Jackson <br>
			  Patient: John Doe
			</div>
			<div class="toptile">
				<button><%= link_to("Notes list", notes_path) %></button>
			  <button><%= link_to("Add Note", new_note_path) %></button>
			</div>
			<div class="toptile">
				<button><%= link_to("Labs list", labs_path) %></button>
			  <button><%= link_to("Add Lab", new_lab_path) %></button>
			</div>
			<div class="toptile">
				<button><%= link_to("Vitals list", vital_signs_path) %></button>
			  <button><%= link_to("Add Vitals", new_vital_sign_path) %></button>
			</div>
			<div class="toptile">
				<button><%= link_to("Meds list", root_path) %></button>
			  <button><%= link_to("Add Med", new_med_path) %></button>   
			</div>
		</div>
		

		

		<div class="rtbar">
			<div class="rttile">
				start stream
			</div>
			<div class="rttile">
				<button><%= link_to("Orders list", orders_path) %></button>
			  <button><%= link_to("Add Order", new_order_path) %></button>
			</div>
			<div class="rttile">
				upload image
			</div>
		</div>

		<div class="content">
			<%= yield %>
		</div>

	</body>
</html>
