

<article>
  <header>
    <h1><%= @med.name %></h1>
  </header>
  <h2>Dose:</h2>
  <p>
    <%= @med.dose %>
  </p>
  <h3>Route:</h3>
  <p>
    <%= @med.route %>
  </p>
  <h3>Dose Timing:</h3>
  <p>
    <%= @med.dose_timing %>
  </p>
  <h3>Ordering Provider:</h3>
  <p>
    <%= @med.ordering_provider %>
  </p>

</article>

<nav>
  <%= link_to("all meds", root_path) %>
  <%= link_to("edit", edit_med_path(@med)) %>
  <%= link_to "delete", med_path(@med), method: :delete, data: { confirm: 'Are you sure?'} %>
</nav>